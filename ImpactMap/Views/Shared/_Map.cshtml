
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>


<div id="map"></div>

<script>
      var map;
      function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
          center: {lat: -34.397, lng: 45.00},
          zoom: 5
        });
      }
</script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDJAzfOpO0EYeXNVVnFpEn0m7EZfo7URLM&callback=initMap"
        async defer></script>